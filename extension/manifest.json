{
  "manifest_version": 2,
  "name": "Nerdcast Insercoes",
  "version": "0.1",

  "content_scripts": [
    {
      "matches": [
        "https://jovemnerd.com.br/nerdcast/*"
      ],
      "js": [
        "third-party/jquery-3.6.0.min.js",
        "third-party/photoswipe.min.js",
        "third-party/photoswipe-ui-default.min.js",
        "content.js"
      ]
    }
  ],

  "web_accessible_resources": [ "assets/*" ]
}

